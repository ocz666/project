<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="activitytest.example.com.thefatlock">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".thelocker"
            android:label="thelocker" />
        <activity
            android:name=".thediet"
            android:label="diet" />
        <activity
            android:name=".theexercise"
            android:label="exercise" />
        <activity
            android:name=".theweight"
            android:label="weight" />
        <activity
            android:name=".records"
            android:label="records" />
        <activity
            android:name=".me"
            android:label="me" />
        <activity android:name=".testweightrecord"></activity>
    </application>

</manifest>